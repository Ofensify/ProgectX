<div class="container-landing">
      <h2>Signup</h2> 
</br>
</br>
<form action="/signup" method="POST" id="form-container">
  <label for="username">Username</label>
  <input id="username_sign" type="text" name="username"  class="form-control" placeholder="JonSnow">
  <br><br>
  <label for="password">Password</label>
  <input id="password_sign" type="password" name="password" class="form-control"  placeholder="Your password">
  <br><br>
  <label for="email">Email</label>
  <input id="email_sign" type="text" name="email" class="form-control" placeholder="Your email">
  

  <% if (typeof message !== "undefined") { %>
    <div class="error-message"><%= message %></div>
  <% } %>
  <br><br>

  <button>Create account</button>
  <br>
  <p class="account-message">
    Do you already have an account?
    <a href="/">Login</a>
  </p>
</form>
</div>